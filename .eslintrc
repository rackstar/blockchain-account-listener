{
  "parser": "@typescript-eslint/parser",
  "extends": ["airbnb-base", "airbnb-typescript/base", "prettier"],
  "plugins": ["prettier", "@typescript-eslint"],
  "rules": {
    "prettier/prettier": ["error"],
  },
  "overrides": [
    {
      "files": ["*.ts"],
      "extends": ["airbnb-base", "airbnb-typescript/base", "prettier"],
      "parserOptions": {
        "project": ["tsconfig.json"]
      }
    }
  ]
}
